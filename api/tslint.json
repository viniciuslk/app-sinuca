{
  "defaultSeverity": "error",
  "extends": ["tslint-config-prettier"],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "prettier": true,
    "no-unused-variable": true,
    "ordered-imports": [
      true,
      {
        "grouped-imports": true,
        "import-sources-order": "case-insensitive",
        "named-imports-order": "case-insensitive"
      }
    ]
  },
  "rulesDirectory": []
}
